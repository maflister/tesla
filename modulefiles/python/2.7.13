#%Module -*- tcl -*-
##
## modulefile
##
proc ModulesHelp { } {

  puts stderr "\tAdds Python 2.7.13 to your environment variables,"
}

module-whatis "adds Python 2.7.13 to your environment variables."

set              version           2.7.13
set              root              /rcc/shared/apps/python/$version

prepend-path      PATH              $root/bin
prepend-path      MANPATH           $root/share/man
prepend-path      LD_LIBRARY_PATH   $root/lib

